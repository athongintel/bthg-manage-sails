div(ng-controller='AdminStockEximController as ctrl' ng-init='ctrl.init()')
    div
        div.row
            div.col-xs-12.col-sm-6
                div.panel-header-title #{__('stock_exim')}

    div.panel.panel-default.main-panel
        div.panel-body(style='background: white')
            uib-tabset(active='active')
                uib-tab()
                    uib-tab-heading
                        i.fas.fa-chart-bar
                        span &nbsp #{__('statistics_')}

                    div.row.margin-top-15px
                        div.col-xs-12.col-sm-4
                            label #{__('select_branch')}
                            branch-selector(global='global')
                uib-tab()
                    uib-tab-heading
                        i.fas.fa-truck
                        span &nbsp #{__('exim_')}

                    div.row.margin-top-15px

